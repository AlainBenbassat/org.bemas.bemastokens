<?xml version="1.0"?>
<extension key="org.bemas.bemastokens" type="module">
  <file>bemastokens</file>
  <name>BEMAStokens</name>
  <description>Adds tokens for BEMAS</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-05-02</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>Adds tokens like current employer address, member contacts...</comments>
  <civix>
    <namespace>CRM/Bemastokens</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
